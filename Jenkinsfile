pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                echo 'Latest code changes are committed.'
            }
        }
        stage('Build') {
            steps {
                echo 'Build successful! Please check the build logs.'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing completed! Please check the logs.'
            }
        }
        
    }
}
